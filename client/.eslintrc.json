{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module",
        "emitWarning": false
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-one-expression-per-line": "warn",
        "react/jsx-filename-extension": "warn",
        "react/prop-types": "warn",
        "no-param-reassign": "warn"
    }
}
